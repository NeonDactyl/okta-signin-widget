{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "react": [
        "node_modules/preact/compat"
      ],
      "react-dom": [
        "node_modules/preact/compat"
      ]
    }
  }
}
